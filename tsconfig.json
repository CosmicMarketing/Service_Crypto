{
    "compilerOptions": {
      "outDir": "./dist",
      "module": "commonjs",
      "target": "ES6",
      "lib": ["DOM", "ES6", "DOM.Iterable", "ScriptHost", "ES2016.Array.Include"],
      "alwaysStrict": true,
      "strict": true,
      "preserveConstEnums": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "esModuleInterop": true,
      "types": ["@cloudflare/workers-types"]
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist"]
  }